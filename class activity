class Person:
  "This is a person class"
  age = 10
  def greet(self):
    print('Hello')

print(Person.age)
print(Person.greet)
print(Person._doc_)

####################
class dog:
    'this is my dog class'
    breed = 'bichon'
    name = 'peanut'

    def bark(self):
        print('bark bark')
print(dog.breed)
print(dog.bark)
print(dog.__doc__)



peanut = dog()
print(peanut.breed)
print(peanut.name)
print(peanut.bark)


################
class ComplexNumber:
    def __init__(self, r=0, i=0, y=0):
        self.real = r
        self.imag = i
        self.attr = y
        def get_data(self):
            print(f'{self.real}+{self.imag}j')
 
 
 Create a new ComplexNumber object
num1 = ComplexNumber(2, 3)
 
 Call get_data() method
 Output: 2+3j
num1.get_data()
 
 Create another ComplexNumber object
 and create a new attribute 'attr'
num2 = ComplexNumber(5)
num2.attr = 10
 
 Output: (5, 0, 10)
print((num2.real, num2.imag, num2.attr))
 
 but c1 object doesn't have attribute 'attr'
 AttributeError: 'ComplexNumber' object has no attribute 'attr'
print(num1.attr)


class code
import csv
with open('red.csv', 'r') as file:
  	reader = csv.reader(file)
  	for each_row in reader:
        	print(each_row)
  
  
  
  class ComplexNumber:
 def _init_(self, r=0, i=0):
  self.real = r
  self.imag = i
 
 def get_data(self):
  print(f'{self.real}+{self.imag}j')
 
 
# Create a new ComplexNumber object
num1 = ComplexNumber(2, 3)
 
# Call get_data() method
# Output: 2+3j
num1.get_data()
 
#Create another ComplexNumber object
#and create a new attribute 'attr'
num2 = ComplexNumber(5)
num2.attr = 10
 
# Output: (5, 0, 10)
print((num2.real, num2.imag, num2.attr))
 
# but c1 object doesn't have attribute 'attr'
# AttributeError: 'ComplexNumber' object has no attribute 'attr'
print(num2.attr)



 
def get_data(self):
  print(f'{self.real}+{self.imag}j')
 
 
